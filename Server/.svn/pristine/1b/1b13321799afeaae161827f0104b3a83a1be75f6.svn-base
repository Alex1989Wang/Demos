package protocol;

enum ENCRYPT_TYPE {
	RC4 = 1;
};

message ExchangeKeyReq {
	required bytes N = 1;
	required int32 E = 2;
}

message ExchangeKeyRes {
	required bytes key = 1;
	required ENCRYPT_TYPE type = 2;
}

message ExchangeKey {
	optional ExchangeKeyReq exchange_key_req = 1;
	optional ExchangeKeyRes exchange_key_res = 2;
}

